# WebRTC STUN/TURN Server Configuration
# Copy this file to .env and fill in your actual credentials

# Twilio TURN Server (Premium, paid)
# Sign up at https://www.twilio.com/
# Go to Console > Programmable Video > Settings > TURN
VITE_TWILIO_TURN_USERNAME=your_twilio_username
VITE_TWILIO_TURN_CREDENTIAL=your_twilio_credential

# Xirsys TURN Server (Premium, paid)
# Sign up at https://xirsys.com/
VITE_XIRSYS_TURN_USERNAME=your_xirsys_username
VITE_XIRSYS_TURN_CREDENTIAL=your_xirsys_credential

# Custom TURN Server (Self-hosted)
VITE_CUSTOM_TURN_SERVER=your-turn-server.com
VITE_CUSTOM_TURN_USERNAME=your_custom_username
VITE_CUSTOM_TURN_CREDENTIAL=your_custom_credential

# Free TURN Server Options (for testing)
VITE_OPENRELAY_TURN_USERNAME=openrelayproject
VITE_OPENRELAY_TURN_CREDENTIAL=openrelayproject

# Numb TURN (free with registration at https://numb.viagenie.ca/)
VITE_NUMB_USERNAME=webrtc@live.com
VITE_NUMB_PASSWORD=muazkh

# Signaling Server Configuration
VITE_SIGNALING_SERVER_URL=https://wuuble.onrender.com
VITE_USE_PRODUCTION_SIGNALING=true

# App Configuration
VITE_APP_MODE=production